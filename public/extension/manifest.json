{
  "manifest_version": 3,
  "name": "New Tab Sound",
  "version": "1.1",
  "permissions": [
    "tabs",
    "offscreen",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Settings"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "modules/audio/audio.mp3"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}